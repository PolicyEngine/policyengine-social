# Copy this to .env and fill in your credentials
# NEVER commit the .env file to GitHub!

# @thepolicyengine account (X/Twitter)
X_THEPOLICYENGINE_API_KEY=your_api_key_here
X_THEPOLICYENGINE_API_SECRET=your_api_secret_here
X_THEPOLICYENGINE_ACCESS_TOKEN=your_access_token_here
X_THEPOLICYENGINE_ACCESS_TOKEN_SECRET=your_access_token_secret_here

# @policyengineus account
X_POLICYENGINEUS_API_KEY=your_api_key_here
X_POLICYENGINEUS_API_SECRET=your_api_secret_here
X_POLICYENGINEUS_ACCESS_TOKEN=your_access_token_here
X_POLICYENGINEUS_ACCESS_TOKEN_SECRET=your_access_token_secret_here

# @policyengineuk account
X_POLICYENGINEUK_API_KEY=your_api_key_here
X_POLICYENGINEUK_API_SECRET=your_api_secret_here
X_POLICYENGINEUK_ACCESS_TOKEN=your_access_token_here
X_POLICYENGINEUK_ACCESS_TOKEN_SECRET=your_access_token_secret_here

# Bluesky accounts (optional)
# Get app passwords from: https://bsky.app/settings/app-passwords
BLUESKY_POLICYENGINE_HANDLE=policyengine.org
BLUESKY_POLICYENGINE_PASSWORD=your_app_password_here

BLUESKY_POLICYENGINEUS_HANDLE=us.policyengine.org
BLUESKY_POLICYENGINEUS_PASSWORD=your_app_password_here

BLUESKY_POLICYENGINEUK_HANDLE=uk.policyengine.org
BLUESKY_POLICYENGINEUK_PASSWORD=your_app_password_here

# Zapier webhooks (optional)
ZAPIER_LINKEDIN_WEBHOOK=your_webhook_url_here
ZAPIER_ALL_WEBHOOK=your_webhook_url_here